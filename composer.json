{
  "name": "tiny-pixel/admin-to-console",
  "type": "wordpress-plugin",
  "version": "1.3.0",
  "license": "MIT",
  "description": "WordPress plugin that send nags to console",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "homepage": "https://github.com/kellymears"
    }
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/admin-to-console/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\AdminToConsole\\": "./src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
