{
  "name": "tiny-pixel/admin-to-console",
  "type": "wordpress-plugin",
  "version": "1.2.0",
  "license": "MIT",
  "description": "WordPress plugin that pipes nags to console",
  "homepage": "https://tinypixel.io/",
  "authors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.io",
      "homepage": "https://github.com/kellymears"
    }
  ],
  "keywords": [
    "utilities",
    "administration"
  ],
  "support": {
    "issues": "https://github.com/pixelcollective/admin-to-console/issues"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\AdminToConsole\\": "./src"
    }
  },
  "require": {
    "php": "^7.1",
    "composer/installers": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
